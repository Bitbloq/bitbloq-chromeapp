{
    "name": "Bitbloq-beta",
    "description": "Bitbloq Chrome app for Beta Environment",
    "version": "1.0.19",
    "manifest_version": 2,
    "author": "BQ",
    "app": {
        "background": {
            "scripts": [
                "background.bundle.js"
            ]
        }
    },
    "externally_connectable": {
        "matches": [
            "http://beta-bitbloq.bq.com/",
            "http://front-next.bitbloq.k8s.bq.com/",
            "http://front-robots.bitbloq.k8s.bq.com/",
            "*://127.0.0.1:*/"
        ]
    },
    "permissions": [
        "serial"
    ],
    "icons": {
        "16": "icons/Bitbloq_16px.png",
        "32": "icons/Bitbloq_32px.png",
        "48": "icons/Bitbloq_48px.png",
        "128": "icons/Bitbloq_128px.png",
        "256": "icons/Bitbloq_256px.png"
    }
}